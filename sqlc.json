{
  "version": "2",
  "sql": [
    {
      "engine": "postgresql",
      "schema": "migrations",
      "queries": ["queries"],
      "codegen": [
        {
          "plugin": "sqlc-gen-rust",
          "out": "src/pgdb",
          "options": {
            "db_crate": "deadpool-postgres",
            "overrides": [
              {
                "db_type": "vector",
                "rs_type": "pgvector::Vector"
              },
              {
                "db_type": "jsonb",
                "rs_type": "serde_json::Value"
              },
              {
                "db_type": "json",
                "rs_type": "serde_json::Value"
              },
              {
                "db_type": "timestamptz",
                "rs_type": "jiff::Timestamp"
              },
              {
                "db_type": "timestamp",
                "rs_type": "jiff::Timestamp"
              }
            ],
            "query_overrides": {
              "GetLayoutState": {
                "row": {
                  "struct": ["#[derive(serde::Serialize, serde::Deserialize)]", "#[serde(rename_all = \"camelCase\")]"]
                }
              }
            }
          }
        }
      ]
    }
  ],
  "plugins": [
    {
      "name": "sqlc-gen-rust",
      "wasm": {
        "url": "https://github.com/casualjim/sqlc-gen-rust/releases/download/v0.1.10-extras.1/sqlc-gen-rust.wasm",
        "sha256": "57deb752c4f3eb9fd8f09e9509365c6a59ada4271ceccdf3e4e9d68b4b05dcbf"
      }
    }
  ]
}
