{
  "mcpServers": {
    "rust-lsp": {
      "args": ["-y", "@p1va/symbols@latest", "run", "rust-analyzer"],
      "command": "bunx",
      "env": {},
      "type": "stdio",
      "disabled": false,
      "alwaysAllow": []
    },
    "typescript-lsp": {
      "args": ["-y", "@p1va/symbols@latest", "run", "bunx", "-y", "typescript-language-server", "--stdio"],
      "command": "bunx",
      "env": {
        "SYMBOLS_PRELOAD_FILES": "assets/js/main.ts",
        "SYMBOLS_DIAGNOSTICS_STRATEGY": "push"
      },
      "type": "stdio",
      "disabled": false,
      "alwaysAllow": []
    }
  }
}
